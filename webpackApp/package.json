{
  "name": "webpackapp",
  "version": "1.0.0",
  "description": "Set up Webpack for Basic Javascript Projects",
  "engines": {
    "node": ">= 16.15.1 <=18.15.0"
  },
  "main": "index.js",
  "scripts": {
    "clean:prod": "rimraf prod/dist",
    "clean:dev": "rimraf dev/dist",
    "build": "webpack --config webpack.config.prod.js",
    "server": "webpack-dev-server --open --config webpack.config.dev.js",
    "build:prod": "npm run clean:prod && npm run build",
    "build:dev": "npm run clean:dev && npm run server"
  },
  "keywords": [],
  "author": "Biswasindhu Mandal",
  "license": "ISC",
  "dependencies": {
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "devDependencies": {
    "css-loader": "^6.7.3",
    "style-loader": "^3.3.1",
    "webpack-dev-server": "^4.11.1"
  }
}
