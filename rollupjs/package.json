{
  "name": "rollupjs",
  "version": "1.0.0",
  "description": "Set up RollupJs for Basic Javascript Projects",
  "engines": {
    "node": ">= 16.15.1 <=18.15.0"
  },
  "type": "module",
  "scripts": {
    "clean": "rimraf build",
    "build:dev": "rollup --config",
    "build:prod": "npm run clean && NODE_ENV=production rollup --config"
  },
  "keywords": ["rollup", "rollup-plugin"],
  "author": "Biswasindhu Mandal",
  "license": "ISC",
  "devDependencies": {
    "rimraf": "^5.0.5",
    "rollup": "^4.6.1",
    "rollup-plugin-serve": "^3.0.0",
    "win-node-env": "^0.6.1"
  }
}
